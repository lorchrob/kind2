node M(x1: int; x2: bool) returns (y1: int; y2: bool)
let
    -- substitute "int" for "T" in N, and then type check the call
    y1 = N<<int>>(x1);

    -- substitute "bool" for "T" in N, and then type check the call
    y2 = N<<bool>>(x2);
