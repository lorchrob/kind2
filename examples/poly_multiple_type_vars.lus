node N<<T1; T2>>(x1: T1; x2: T2) returns (y1: T1; y2: T2) 
let 
    -- Should be typable for any such T   
    y1 = x1 -> pre x1;
    y2 = x2 -> pre x2;
tel

node M(x1: int; x2: bool) returns (y1: int; y2: bool)
let
    -- substitute "int" for "T" in N, and then type check the call
    y1, y2 = N<<int; bool>>(x1, x2);
tel